{
  "name": "server",
  "version": "2.1.2",
  "description": "RioBus server application",
  "main": "dist/index.js",
  "scripts": {
    "configure": "tsd reinstall && tsd rebundle",
    "test": "node ./node_modules/gulp/bin/gulp.js buildTest && node_modules/mocha/bin/mocha .compiled/test/**/*",
    "build": "node ./node_modules/gulp/bin/gulp.js build",
    "release": "node ./node_modules/gulp/bin/gulp.js run release",
    "start": "node ./node_modules/gulp/bin/gulp.js build && node ./node_modules/gulp/bin/gulp.js run",
    "deploy": "node ./node_modules/gulp/bin/gulp.js build && node ./node_modules/gulp/bin/gulp.js release && node ./node_modules/gulp/bin/gulp.js run-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/riobus/server"
  },
  "keywords": [
    "riobus",
    "transporte",
    "mobilidade",
    "publico",
    "rj",
    "onibus",
    "typescript",
    "nodejs",
    "node",
    "desktop",
    "server",
    "wrapper",
    "infrastructure"
  ],
  "author": "Fred Souza <fredericoamsouza@gmail.com> (http://fmsouza.github.io)",
  "license": "http://github.com/RioBus/server/blob/master/LICENSE",
  "bugs": {
    "url": "http://github.com/RioBus/server/issues"
  },
  "homepage": "http://github.com/RioBus/server",
  "dependencies": {
    "arangojs": "^3.8.1",
    "compression": "^1.4.4",
    "deasync": "^0.1.0",
    "emailjs": "^0.3.16",
    "express": "^4.12.4",
    "fs-extra": "^0.18.4",
    "nodealytics": "0.0.6",
    "request": "^2.57.0"
  },
  "devDependencies": {
    "gulp": "^3.9.0",
    "gulp-load-plugins": "^0.10.0",
    "gulp-shell": "^0.4.2",
    "gulp-typescript": "^2.7.6",
    "gulp-uglify": "^1.2.0",
    "mocha": "^2.2.5",
    "tsd": "^0.6.1"
  }
}
